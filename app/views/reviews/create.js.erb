<% if @review %>
    var reviews = $("#reviews");
    if (reviews.length > 0) {
        var child = '<%= escape_javascript(render @review) %>';
        reviews.prepend(child);
    }
<% end %>

<% if @feed %>
    var feeds = $("#feeds");
    if (feeds.length > 0) {
        var child = '<%= escape_javascript(render @feed) %>';
        feeds.prepend(child);
    }
<% end %>