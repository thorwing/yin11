<div class="grid_9 to_left">
    <%= form_for(@page) do |f| %>
      <%= render "shared/error_messages", target: @page %>

      <div class="field">
        <%= f.label :title, t("general.title") %><br />
        <%= f.text_field :title %>
      </div>
      <div class="field">
        <%= f.label :content, t("general.content") %><br />
        <%= f.text_area :content %>
      </div>
      <div class="actions">
        <%= f.submit t("general.submit") %>
      </div>
    <% end %>
</div>

<div class="grid_3 to_right">
 <div>
    <%= hidden_field_tag "authenticity_token", form_authenticity_token, :id => "tokentag" %>
    <div id="uploader" class="fl mr10" data-debug = true></div>
  </div>
  <div id="images_container" class="block fl m5" ></div>
  <div class="clearfix"></div>
</div>