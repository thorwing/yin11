<div id="<%= vendor.id.to_s %>" class="block round">
  <%= content_tag :small, vendor.category, :class => "right_column" %>
  <div class="left_column">
    <%= link_to vendor.name, vendor %><br/>
    <div class="actions">
        <%= render "shared/follow_field", :target => vendor %>
    </div>
  </div>
</div>